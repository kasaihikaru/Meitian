<div class="row">
	<div class="col-xs-12 col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8" >
			<table class="table table-sm">
				<thead class="border-b-double">
					<tr>
						<% if user_signed_in? && paper.user_id == current_user.id %>
							<th scope="col" class=""></th>
						<% end %>
						<th scope="col" class="">日</th>
						<% if user_signed_in? && paper.user_id == current_user.id %>
							<th scope="col" class=""></th>
						<% end %>
						<th scope="col" class="">中</th>
						<th scope="col" class="">Pin</th>
					</tr>
				</thead>
				<tbody class="">
					<% @sentences.each do |sentence| %>
						<% sentence.s_words.active.each do |word| %>
							<tr id="s_word_row_<%= word.id %>">
								<%= render :partial => 'render_for_word/s_word_row_ja', :locals=>{paper: paper, word: word} %>
							</tr>
						<% end %>
					<% end %>
				</tbody>
			</table>
	</div>
</div>